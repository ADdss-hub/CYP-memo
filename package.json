{
  "name": "cyp-memo",
  "version": "1.9.1",
  "type": "module",
  "private": true,
  "description": "CYP-memo 容器备忘录系统",
  "homepage": "https://github.com/ADdss-hub/CYP-memo",
  "author": {
    "name": "CYP",
    "email": "nasDSSCYP@outlook.com"
  },
  "license": "MIT",
  "packageManager": "pnpm@10.11.0",
  "scripts": {
    "dev": "node scripts/dev.js",
    "dev:server": "pnpm --filter @cyp-memo/server dev",
    "dev:all": "concurrently \"pnpm dev:server\" \"pnpm dev\"",
    "build": "node scripts/build.js",
    "test": "pnpm -r test",
    "lint": "pnpm -r lint",
    "format": "pnpm -r format",
    "release": "node scripts/release.js",
    "release:patch": "node scripts/release.js patch",
    "release:minor": "node scripts/release.js minor",
    "release:major": "node scripts/release.js major",
    "version:info": "node scripts/verify-version.js"
  },
  "devDependencies": {
    "concurrently": "^8.2.2"
  },
  "engines": {
    "node": ">=20.19.6",
    "pnpm": ">=10"
  },
  "pnpm": {
    "overrides": {
      "node-gyp": "10.3.1"
    },
    "onlyBuiltDependencies": [
      "better-sqlite3",
      "keytar",
      "electron",
      "esbuild",
      "sharp"
    ]
  }
}
